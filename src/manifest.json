{
    "manifest_version": 2,
    "name": "Youtube Yellow Ward",
    "version": "1.0.0",
    "description": "Extension to watch Youtube's video in Chrome tab",
    "content_scripts": [
      {
        "matches": [
          "*://*/*"
        ],
        "js": [
          "js/main.js"
        ],
        "css": [
            "css/app.css"
          ],
        "run_at": "document_end"
      }
    ],
    "permissions": [
      "storage"
    ],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
  }